{
  "extends": "@backstage/cli/config/tsconfig.json",
  "include": [
    "packages/*/src",
    "plugins/*/src",
    "plugins/*/dev",
    "plugins/*/migrations"
  ],
  "compilerOptions": {
    "outDir": "dist-types",
    "rootDir": ".",
    "paths": {
      "@backstage/*/alpha": ["./node_modules/@backstage/*"],
      "@backstage/*/beta": ["./node_modules/@backstage/*"]
    },
    "useUnknownInCatchVariables": false
  }
}
